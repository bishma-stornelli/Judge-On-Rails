#solve_form
  = form_for(:marked_problem, url: marked_problem_path(@problem.id), method: :put, remote: true) do |f|
    %table
      %tr
        %td Submit your solution here:
        %td= f.file_field :solution, class: "input-file"
      %tr
        %td= f.hidden_field "problem_id", value: @problem.id
        %td= f.submit "Solve", class: "submit"
  
